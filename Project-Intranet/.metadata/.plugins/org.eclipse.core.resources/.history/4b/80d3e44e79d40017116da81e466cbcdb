
public enum Degree {
	BACHELOR{
		public String toString() {
			return 	"\n 1) " + BACHELOR;
		}
		
	}, MASTER{
		public String toString() {
			return 	"\n 2) " + MASTER;
		}
	}, PHD{
		public String toString() {
			return 	"\n 3) " + PHD;
		}
	}, DOCTOR{
		public String toString() {
			return 	"\n 4) " + DOCTOR;
		}
	};
	
	 
	}
}
